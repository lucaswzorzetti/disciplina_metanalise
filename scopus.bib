@ARTICLE{Vangansbeke2015,
author={Vangansbeke, D. and Audenaert, J. and Nguyen, D.T. and Verhoeven, R. and Gobin, B. and Tirry, L. and De Clercq, P.},
title={Diurnal temperature variations affect development of a herbivorous arthropod pest and its predators},
journal={PLoS ONE},
year={2015},
volume={10},
