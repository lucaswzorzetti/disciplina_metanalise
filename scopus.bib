@ARTICLE{Tangkawanit2018663,
author={Tangkawanit, U. and Hinmo, N. and Khlibsuwan, W.},
title={Role of different habitats for the functional response of Crytorrhinus lividipennis (Hemiptera: Miridae) on Nilaparvata lugens (Hemiptera: Delphacidae)},
