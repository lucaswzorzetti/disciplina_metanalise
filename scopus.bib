@ARTICLE{Anderson2019433,
author={Anderson, K.L. and Druckenmiller, P.S. and Erickson, G.M. and Maxwell, E.E.},
title={Skeletal microstructure of Stenopterygius quadriscissus (Reptilia, Ichthyosauria) from the Posidonienschiefer (Posidonia Shale, Lower Jurassic) of Germany},
journal={Palaeontology},
year={2019},
volume={62},
