@ARTICLE{Bonaccorso200343,
author={Bonaccorso, F.J. and McNab, B.K.},
title={Standard energetics of leaf-nosed bats (Hipposideridae): Its relationship to intermittent- and protracted-foraging tactics in bats and birds},
