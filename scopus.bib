@ARTICLE{Zhang20201028,
author={Zhang, P. and van Leeuwen, C.H.A. and Bogers, D. and Poelman, M. and Xu, J. and Bakker, E.S.},
title={Ectothermic omnivores increase herbivory in response to rising temperature},
