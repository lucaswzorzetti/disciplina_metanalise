@ARTICLE{Bar-Ziv2018181,
author={Bar-Ziv, M.A. and Scharf, I.},
title={Thermal acclimation is not induced by habitat-of-origin, maintenance temperature, or acute exposure to low or high temperatures in a pit-building wormlion (Vermileo sp.)},
journal={Journal of Thermal Biology},
